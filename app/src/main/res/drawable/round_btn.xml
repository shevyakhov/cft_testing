<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true">
        <shape>
            <gradient android:angle="135" android:centerColor="?attr/colorPrimary" android:endColor="?attr/colorSecondary" android:startColor="?attr/colorTertiary" />
            <stroke android:width="1dp" android:color="@color/black" />
            <corners android:radius="20dp" />
        </shape>
        <ripple android:color="@color/black" android:radius="20dp" />
    </item>


</selector>
